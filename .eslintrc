{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "quotes": "off",
    "eol-last": "off",
    "indent": "off",
"spaced-comment": "off",
"key-spacing": "off",
"max-len": "off",
"object-curly-spacing": "off",
    "comma-spacing": "off",
    "space-before-blocks": "off",
    "no-multiple-empty-lines": "off"

  },
  "extends": ["eslint:recommended", "google"]
}